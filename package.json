{
  "name": "buttplug-relay-client",
  "version": "1.0.0",
  "description": "buttplug relay",
  "author": "blackspherefollower <darktech@black-sphere.co.uk>",
  "private": true,
  "scripts": {
    "start:server": "npm run build:config && nodemon --watch src/server --exec \"ts-node -P tsconfig.server.json\" src/server/server.ts",
    "start": "npm run build:config && webpack-dev-server --hot --config build/webpack.base.js",
    "start:remote": "npm run build:config && webpack-dev-server --hot --host 192.168.123.3 --config build/webpack.base.js",
    "start:secure": "npm run build:config && webpack-dev-server --hot --https --config build/webpack.base.js",
    "start:remote:secure": "npm run build:config && webpack-dev-server --hot --https --host 192.168.123.3 --config build/webpack.base.js",
    "build:config": "node build/generate_config.js",
    "build:dev": "npm run build:config && webpack --progress --hide-modules --config build/webpack.base.js",
    "build": "npm run build:config && webpack --progress --hide-modules --config build/webpack.production.js && tsc -p tsconfig.server.json",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "webpack-defaults": "webpack-defaults",
    "build:main": "npm run build:config && tsc -p tsconfig.server.json",
    "pretest": "npm run build:main",
    "deploy": "trash deploy && mkdir -p deploy && cp index.html deploy && cp -r dist deploy"
  },
  "dependencies": {
    "@types/express-serve-static-core": "^4.17.9",
    "buttplug": "^0.13.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/express-ws": "^3.0.0",
    "@types/node": "^14.0.25",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "class-transformer": "^0.2.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.5.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "html-loader": "^1.1.0",
    "markdown-loader": "^5.1.0",
    "nodemon": "^2.0.4",
    "noop-loader": "^1.0.0",
    "null-loader": "^4.0.0",
    "project-name-generator": "^2.1.7",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "serialize-javascript": "^4.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "trash": "^6.1.1",
    "trash-cli": "^3.0.0",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.6.11",
    "vue-awesome": "^4.1.0",
    "vue-buttplug-material-component": "^1.0.2",
    "vue-loader": "^15.9.3",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vue-slider-component": "^3.2.2",
    "vue-template-compiler": "^2.6.11",
    "vue-touch": "^2.0.0-beta.4",
    "vuetify": "^2.3.6",
    "vuetify-loader": "^1.6.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "pre-push": []
}
